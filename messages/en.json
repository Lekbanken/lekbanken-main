      "metadata": {
        "title": "Terms of Service - Lekbanken",
        "description": "Terms of service and rules for using Lekbanken"
      },
      "title": "Terms of Service",
      "sections": {
        "introduction": {
          "title": "1. Introduction",
          "content": "Welcome to Lekbanken! These terms of service (\"Terms\") govern your use of the Lekbanken platform and our services. By using our services, you agree to these terms."
        },
        "usage": {
          "title": "2. Use of the Service",
          "eligibility": {
            "title": "2.1 Eligibility",
            "content": "To use Lekbanken, you must be at least 18 years old or have permission from a guardian. If you use the service on behalf of an organization, you warrant that you have authority to enter into this agreement on behalf of the organization."
          },
          "security": {
            "title": "2.2 Account Security",
            "content": "You are responsible for keeping your login credentials secure and for all activity that occurs under your account. You must immediately notify us if you discover unauthorized use of your account."
          },
          "acceptable": {
            "title": "2.3 Acceptable Use",
            "intro": "You agree not to:",
            "illegal": "Use the service for illegal purposes",
            "unauthorized": "Attempt to gain unauthorized access to the system",
            "malware": "Upload harmful content or viruses",
            "harassment": "Harass, threaten or insult other users",
            "spam": "Transmit spam or unsolicited communications",
            "ip": "Violate others' intellectual property rights"
          }
        },
        "content": {
