{
  "name": "lekbanken-main",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prebuild": "npm run validate:i18n",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "validate:i18n": "node scripts/validate-messages-json.mjs",
    "pretest": "npm run validate:i18n",
    "test": "node tests/browse/search-helpers.test.cjs && npm run test:csv-header-sync && npm run test:trigger-alias-roundtrip && npm run test:legendary-json-roundtrip",
    "test:csv-header-sync": "ts-node -T --skip-project --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" scripts/verify-csv-header-sync.ts",
    "test:trigger-alias-roundtrip": "ts-node -T --skip-project --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" scripts/test-trigger-order-alias-roundtrip.ts",
    "test:legendary-json-roundtrip": "ts-node -T --skip-project --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" scripts/test-legendary-json-roundtrip.ts",
    "db:types": "npx --yes supabase gen types typescript --local > types/supabase.ts",
    "db:types:remote": "npx --yes supabase gen types typescript --linked > types/supabase.ts",
    "db:verify": "echo 'Run scripts/verify-migrations.sql in Supabase SQL Editor'",
    "db:check-any": "pwsh -File scripts/find-any-casts.ps1",
    "upload:utmarkelser": "ts-node -T --skip-project --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" scripts/upload-utmarkelser.ts",
    "sync:utmarkelser:local": "ts-node -T --skip-project --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" scripts/sync-utmarkelser-local.ts",
    "smoke:licensing": "ts-node -T --skip-project --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" scripts/smoke-licensing.ts",
    "inventory": "pwsh -File scripts/generate-inventory-v3.ps1",
    "inventory:db": "pwsh -File scripts/generate-inventory-v3.ps1 -DbOnly",
    "inventory:app": "pwsh -File scripts/generate-inventory-v3.ps1 -Domain app",
    "inventory:admin": "pwsh -File scripts/generate-inventory-v3.ps1 -Domain admin",
    "inventory:marketing": "pwsh -File scripts/generate-inventory-v3.ps1 -Domain marketing",
    "inventory:sandbox": "pwsh -File scripts/generate-inventory-v3.ps1 -Domain sandbox",
    "inventory:shared": "pwsh -File scripts/generate-inventory-v3.ps1 -Domain shared"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
